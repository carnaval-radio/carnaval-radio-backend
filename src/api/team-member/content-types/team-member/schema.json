{
  "kind": "collectionType",
  "collectionName": "team",
  "info": {
    "singularName": "team-member",
    "pluralName": "team",
    "displayName": "Team",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "type": "string"
    },
    "Story": {
      "type": "blocks"
    },
    "Phone": {
      "type": "string"
    },
    "Birthdate": {
      "type": "date"
    },
    "DateJoined": {
      "type": "datetime"
    },
    "NickName": {
      "type": "string"
    },
    "Photo": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "Email": {
      "type": "email"
    },
    "City": {
      "type": "string"
    },
    "Role": {
      "type": "string"
    },
    "Color": {
      "type": "customField",
      "regex": "^#([A-Fa-f0-9]{6}|[A-Fa-f0-9]{3})$",
      "customField": "plugin::color-picker.color"
    },
    "Slug": {
      "type": "uid",
      "targetField": "Name"
    }
  }
}
